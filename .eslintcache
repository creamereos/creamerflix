\n        history: { push }\n      } = this.props;\n      const { isMovie } = this.state;\n      const parsedId = parseInt(id);\n      if (isNaN(parsedId)) {\n        return push(\"/\");\n      }\n      let result = null;\n      try {\n        if (isMovie) {\n          ({ data: result } = await movieApi.movieDetail(parsedId));\n        } else {\n          ({ data: result } = await tvApi.tvDetail(parsedId));\n        }\n      } catch {\n        this.setState({ error: \"Can't find anything.\" });\n      } finally {\n        this.setState({ loading: false, result });\n      }\n    }\n  \n    render() {\n      const { result, error, loading } = this.state;\n      return <DetailPresenter result={result} error={error} loading={loading} />;\n    }\n  }","/Users/CREAMer/Desktop/creamerflix/src/Routes/Detail/DetailPresenter.js",[],"/Users/CREAMer/Desktop/creamerflix/src/Components/Section.js",[],"/Users/CREAMer/Desktop/creamerflix/src/Components/Loader.js",["105"],"import React from \"react\";\nimport styled from \"styled-components\";\n\nconst Container = styled.div`\n    height: 100vh;\n    width: 100vw;\n    display: flex;\n    justify-content: center;\n    font-size: 28px;\n    margin-top: 20px;\n`;\n\nexport default () => (\n    <Container>\n        <span role=\"img\" aria-label=\"Loading\">\n           ⏱ Loading...\n        </span>\n    </Container>);\n\n","/Users/CREAMer/Desktop/creamerflix/src/Components/Message.js",[],"/Users/CREAMer/Desktop/creamerflix/src/Components/Poster.js",[],{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","severity":1,"message":"111","line":9,"column":1,"nodeType":"112","endLine":23,"endColumn":3},{"ruleId":"113","severity":1,"message":"114","line":4,"column":8,"nodeType":"115","messageId":"116","endLine":4,"endColumn":15},{"ruleId":"110","severity":1,"message":"117","line":6,"column":1,"nodeType":"112","endLine":45,"endColumn":2},{"ruleId":"110","severity":1,"message":"117","line":5,"column":1,"nodeTyp